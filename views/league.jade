extends layout
block content
  body
    h1 Teams
    p Team 1
    p Team 2
    p Team 3
    h1 Stocks
    p Apple
    p Starbucks
    p GE
    form(method='POST' action='/createteam')
          div.form-group
            label(for='teamname') Name:
            input#name.form-control(type='text', placeholder='Write your team name' name='teamname')
          button.btn.btn-primary(type='submit') Create Team
